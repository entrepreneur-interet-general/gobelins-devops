APP_NAME=Gobelins
APP_ENV=local
APP_KEY={{ dotenv.gobelins_app_key }}
APP_DEBUG=true
DEBUGBAR_ENABLED=false
APP_URL=http://gobelins.test

LOG_CHANNEL=stack

DB_CONNECTION=pgsql
DB_HOST=127.0.0.1
DB_PORT=5432
DB_DATABASE=gobelins
DB_USERNAME=postgres
DB_PASSWORD={{ dotenv.gobelins_postgres_password}}

BROADCAST_DRIVER=log
CACHE_DRIVER=file
SESSION_DRIVER=file
SESSION_LIFETIME=120
QUEUE_DRIVER=sync

REDIS_HOST=127.0.0.1
REDIS_PASSWORD=null
REDIS_PORT=6379

MAIL_DRIVER=smtp
MAIL_HOST=smtp.mailtrap.io
MAIL_PORT=2525
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_ENCRYPTION=null

PUSHER_APP_ID=
PUSHER_APP_KEY=
PUSHER_APP_SECRET=
PUSHER_APP_CLUSTER=mt1

MIX_PUSHER_APP_KEY="${PUSHER_APP_KEY}"
MIX_PUSHER_APP_CLUSTER="${PUSHER_APP_CLUSTER}"

DATASOURCE_BASEURI=http://gobelins-datasource.test

# Relative to Laravel storage folder.
MEDIA_STORAGE_PATH=app/media

MIX_COLLECTION_DSN="${APP_URL}/rechercher"